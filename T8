#include <stdio.h>

int main() {
    int n, factorial = 1, i, t1 = 0, t2 = 1, nextTerm;

    printf("Enter a number: ");
    scanf("%d", &n);

    for (i = 1; i <= n; i++) {
        factorial *= i;
    }
    printf("Factorial of %d = %d\n", n, factorial);

    int isPrime = 1;  
    if (n <= 1) isPrime = 0;  

    for (i = 2; i <= n / 2; i++) {
        if (n % i == 0) {
            isPrime = 0; 
            break;
        }
    }


    if (isPrime) {
        printf("%d is a prime number.\n", n);
        printf("Fibonacci series till %d: ", n);
        for (i = 1; i <= n; i++) {
            printf("%d ", t1);
            nextTerm = t1 + t2;
            t1 = t2;
            t2 = nextTerm;
        }
        printf("\n");
    } else {
        printf("%d is not a prime number.\n", n);
    }

    return 0; 
}
